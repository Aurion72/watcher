{
  "name": "aurion72/watcher",
  "description": "A watcher",
  "license": "MIT",
  "keywords": [
    "watcher"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Anthony Aslangul",
      "email": "anthony.aslangul@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Aurion72\\Watcher\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aurion72\\Watcher\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aurion72\\Watcher\\WatcherServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
